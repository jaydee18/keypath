# Ignore core and contributed modules, themes, and profiles
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

# Ignore uploaded files and temporary files
/web/sites/*/files/
/web/sites/*/private/
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/services.yml
/web/sites/*/development.services.yml
/web/sites/*/settings.php.e

# Ignore local development files and directories
/web/sites/*/development/
/web/sites/*/dev.settings.php

# Ignore logs and generated files
/web/sites/*/logs/
/web/sites/*/cache/
/web/sites/*/phpunit.xml

# Ignore Drush-generated files
/drush/contrib/

# Ignore configuration export directories
/config/sync/

# Ignore composer-related files
/vendor/

# Ignore database dump files
*.sql

# Ignore PhpStorm IDE files
.idea/

# Ignore Visual Studio Code IDE files
.vscode/

# Ignore DDEV local env files
.ddev/

# Ignore macOS metadata files
.DS_Store

# Ignore Node.js dependencies
/node_modules/
/package-lock.json

# Ignore files generated by PHPUnit tests
/core/phpunit.xml.dist
/web/core/phpunit.xml.dist
/web/core/tests/Drupal/FunctionalJavascriptTests/AllTests.php
/web/core/tests/Drupal/FunctionalTests/AllTests.php
/web/core/tests/Drupal/FunctionalJavascriptTests/Settings.php
/web/core/tests/Drupal/FunctionalTests/Settings.php


# Ignore automated deployment scripts and CI/CD files
/.env
/.env.example
/web/.ht.router.php
/.platform/
/.platform.app.yaml
/.platform/services.yaml
/.platform/routes.yaml
